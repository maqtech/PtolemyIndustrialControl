<?xml version="1.0" standalone="no"?>
<!DOCTYPE model PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<model name="top" class="ptolemy.actor.TypedCompositeActor">
  <director class="ptolemy.domains.sdf.kernel.SDFDirector">
<parameter name="defaultIerations" value="1"/>
  </director>
  <entity name="const" class="ptolemy.actor.lib.Const">
    <property name="value" value="readFile(&quot;modulation.xml&quot;)"/>
    <port name="output"></port>
    <rendition class="ptolemy.moml.Icon">
      <location value="100, 100"/>
    </rendition>
  </entity>
  <entity name="display" class="ptolemy.actor.gui.Display">
    <port name="input"></port>
    <rendition class="ptolemy.moml.Icon">
      <location value="200, 100"/>
    </rendition>
  </entity>

  <!-- FIXME: Link parameters with on-screen controls here. -->

  <relation name="r1" class="ptolemy.actor.TypedIORelation">
    <vertex name="v1"><location value="120, 100"/></vertex>
    <vertex name="v2" pathTo="v1"><location value="120,120"/></vertex>
    <vertex name="v3" pathTo="v2"><location value="180,120"/></vertex>
    <vertex name="v4" pathTo="v3"><location value="180,100"/></vertex>
  </relation>
  <link port="const.output" relation="r1" vertex="v1"/>
  <link port="display.input" relation="r1" vertex="v4"/>
</model>
