<?xml version="1.0" standalone="no"?>
<!DOCTYPE class PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<class name="SenderController" extends="ptolemy.domains.fsm.kernel.FSMActor">
    <property name="initialStateName" class="ptolemy.kernel.util.StringAttribute" value="Connecting">
    </property>
    <property name="_nonStrictMarker" class="ptolemy.kernel.util.Attribute">
    </property>
    <property name="_createdBy" class="ptolemy.kernel.util.VersionAttribute" value="2.1-devel">
    </property>
    <property name="_vergilSize" class="ptolemy.actor.gui.SizeAttribute" value="[600, 454]">
    </property>
    <property name="_vergilLocation" class="ptolemy.actor.gui.LocationAttribute" value="[106, 116]">
    </property>
    <port name="connected" class="ptolemy.actor.TypedIOPort">
        <property name="input"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="58.0, 149.0">
        </property>
    </port>
    <port name="next" class="ptolemy.actor.TypedIOPort">
        <property name="output"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="528.0, 94.0">
        </property>
    </port>
    <port name="error" class="ptolemy.actor.TypedIOPort">
        <property name="input"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="42.25, 71.0">
        </property>
    </port>
    <entity name="Connecting" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute" value="Connect">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="155.0, 57.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <entity name="Failed" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="425.0, 158.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <entity name="Sending" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute" value="Send">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="421.0, 52.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <relation name="ctrlTr1" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="connected_isPresent">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute" value="next = true">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="PI/5.0">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <relation name="ctrlTr2" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="error_isPresent">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="PI/5.0">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <link port="Connecting.outgoingPort" relation="ctrlTr1"/>
    <link port="Connecting.outgoingPort" relation="ctrlTr2"/>
    <link port="Failed.incomingPort" relation="ctrlTr2"/>
    <link port="Sending.incomingPort" relation="ctrlTr1"/>
</class>
