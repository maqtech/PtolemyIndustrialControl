        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute" value="count = count - 1">
        </property>
    </relation>
    <relation name="conTr6" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="ack_isPresent &amp;&amp; ack != -1 &amp;&amp; expired_isPresent &amp;&amp; count != 0">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
