<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="wireless" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
  <doc>Wireless/Sensor network actors</doc>

  <property name="AcousticChannel" class="ptolemy.domains.wireless.lib.AcousticChannel">
  </property>

  <property name="CircularRangeChannel" class="ptolemy.domains.wireless.lib.CircularRangeChannel">
  </property>

  <property name="DelayChannel" class="ptolemy.domains.wireless.lib.DelayChannel">
  </property>

  <property name="RadioChannel" class="ptolemy.domains.wireless.lib.RadioChannel">
  </property>

  <property name="WirelessDirector" class="ptolemy.domains.wireless.kernel.WirelessDirector">
  </property>

  <property name="WirelessChannel" class="ptolemy.domains.wireless.kernel.WirelessChannel">
  </property>
  
  <entity name="SoundSensor" class="ptolemy.domains.wireless.demo.WirelessSoundDetection.SoundSensor">
  </entity>
  
  <entity name="WirelessComposite" class="ptolemy.domains.wireless.kernel.WirelessComposite">
	<!-- The inside MUST have a director, so we create one. -->
	<property name="DEDirector" class="ptolemy.domains.de.kernel.DEDirector"/>
  </entity>

      </group>
    ?>
  </configure>
</entity>
