<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<class name="one_mode" extends="ptolemy.actor.TypedCompositeActor">
    <property class="ptolemy.domains.giotto.kernel.GiottoDirector">
    </property>

    <port name="output1" class="ptolemy.actor.TypedIOPort">
      <property name="output"/>
    </port>
    <port name="output2" class="ptolemy.actor.TypedIOPort">
      <property name="output"/>
    </port>
    <entity name="ramp1" class="ptolemy.actor.lib.Ramp">
        <property name="init" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="step" class="ptolemy.data.expr.Parameter" value="1">
        </property>
	<property name="frequency" class="ptolemy.data.expr.Parameter" value="2">
	</property>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="trigger" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <entity name="ramp2" class="ptolemy.actor.lib.Ramp">
        <property name="init" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="step" class="ptolemy.data.expr.Parameter" value="1">
        </property>
	<property name="frequency" class="ptolemy.data.expr.Parameter" value="2">
	</property>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="trigger" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <relation name="relation6" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="relation7" class="ptolemy.actor.TypedIORelation">
    </relation>
    <link port="ramp1.output" relation="relation6"/>
    <link port="output1" relation="relation6"/>
    <link port="ramp2.output" relation="relation7"/>
    <link port="output2" relation="relation7"/>
</class>



