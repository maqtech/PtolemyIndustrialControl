<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="Monitor" class="ptolemy.actor.TypedCompositeActor">
    <property name="_createdBy" class="ptolemy.kernel.util.VersionAttribute" value="2.1-devel">
    </property>
    <doc>Discrete Event Monitor of the two cars.</doc>
    <property name="DEDirector" class="ptolemy.domains.de.kernel.DEDirector">
        <property name="startTime" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="stopTime" class="ptolemy.data.expr.Parameter" value="1.7976931348623E308">
        </property>
        <property name="stopWhenQueueIsEmpty" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="synchronizeToRealTime" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="isCQAdaptive" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="minBinCount" class="ptolemy.data.expr.Parameter" value="2">
        </property>
        <property name="binCountFactor" class="ptolemy.data.expr.Parameter" value="2">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="100.0, 34.5">
        </property>
    </property>
    <property name="_vergilSize" class="ptolemy.actor.gui.SizeAttribute" value="[600, 400]">
    </property>
    <property name="_vergilLocation" class="ptolemy.actor.gui.LocationAttribute" value="[104, 127]">
    </property>
    <entity name="leadingCar" class="ptolemy.domains.ct.demo.jspaces.CarTracking.DERealTimeSubscriber">
        <property name="jspaceName" class="ptolemy.data.expr.Parameter" value="&quot;JavaSpaces&quot;">
        </property>
        <property name="entryName" class="ptolemy.data.expr.Parameter" value="&quot;A&quot;">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="120.0, 120.0">
        </property>
        <port name="output" class="ptolemy.domains.de.kernel.DEIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="followingCar" class="ptolemy.domains.ct.demo.jspaces.CarTracking.DERealTimeSubscriber">
        <property name="jspaceName" class="ptolemy.data.expr.Parameter" value="&quot;JavaSpaces&quot;">
        </property>
        <property name="entryName" class="ptolemy.data.expr.Parameter" value="&quot;B&quot;">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="120.0, 280.0">
        </property>
        <port name="output" class="ptolemy.domains.de.kernel.DEIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="alarm" class="ptolemy.domains.ct.demo.jspaces.CarTracking.TrackingAlarm">
        <property name="closeThreshold" class="ptolemy.data.expr.Parameter" value="15.0">
        </property>
        <property name="awayThreshold" class="ptolemy.data.expr.Parameter" value="10.0">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="315.0, 194.5">
        </property>
        <port name="leaderPosition" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="followerPosition" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="tooClose" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="tooFar" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="closePublisher" class="ptolemy.domains.ct.demo.jspaces.CarTracking.RealTimePublisher">
        <property name="jspaceName" class="ptolemy.data.expr.Parameter" value="&quot;JavaSpaces&quot;">
        </property>
        <property name="entryName" class="ptolemy.data.expr.Parameter" value="&quot;TooClose&quot;">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="468.0, 109.5">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
    </entity>
    <entity name="farPublisher" class="ptolemy.domains.ct.demo.jspaces.CarTracking.RealTimePublisher">
        <property name="jspaceName" class="ptolemy.data.expr.Parameter" value="&quot;JavaSpaces&quot;">
        </property>
        <property name="entryName" class="ptolemy.data.expr.Parameter" value="&quot;TooFar&quot;">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="461.0, 279.5">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
    </entity>
    <entity name="display" class="ptolemy.actor.lib.gui.TimedPlotter">
        <property name="fillOnWrapup" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="legend" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="startingDataset" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="_location" class="ptolemy.moml.Location" value="526.5, 193.5">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
        <configure source="DEplotConfigure.xml"><![CDATA[
<?xml version="1.0" standalone="no"?>
      	<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD PlotML 1//EN"
           "http://ptolemy.eecs.berkeley.edu/xml/dtd/PlotML_1.dtd">
      	<plot>
        <size width="450" height="300"/>
        <title>Relative Positions</title>
      	</plot>
        ]]>
        </configure>
    </entity>
    <relation name="r0" class="ptolemy.actor.TypedIORelation">
        <vertex name="vertex" class="ptolemy.moml.Vertex" value="240.0, 120.0">
        </vertex>
    </relation>
    <relation name="r1" class="ptolemy.actor.TypedIORelation">
        <vertex name="vertex" class="ptolemy.moml.Vertex" value="240.0, 280.0">
        </vertex>
    </relation>
    <relation name="r2" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="r3" class="ptolemy.actor.TypedIORelation">
    </relation>
    <link port="leadingCar.output" relation="r0"/>
    <link port="followingCar.output" relation="r1"/>
    <link port="alarm.leaderPosition" relation="r0"/>
    <link port="alarm.followerPosition" relation="r1"/>
    <link port="alarm.tooClose" relation="r2"/>
    <link port="alarm.tooFar" relation="r3"/>
    <link port="closePublisher.input" relation="r2"/>
    <link port="farPublisher.input" relation="r3"/>
    <link port="display.input" relation="r0"/>
    <link port="display.input" relation="r1"/>
</entity>
