<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="synchronous reactive" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
<doc>Synchronous-reactive actors</doc>

<entity name="Absent" class="ptolemy.domains.sr.lib.Absent">
    <property name="outputType">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="boolean" value="boolean"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="complex" value="complex"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="double" value="double"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="fix" value="fix"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="int" value="int"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="long" value="long"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="string" value="string"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
</entity>

<entity name="InstantaneousDialogGenerator" class="ptolemy.domains.sr.lib.InstantaneousDialogGenerator">
</entity>

<entity name="NonStrictDelay" class="ptolemy.domains.sr.lib.NonStrictDelay">
</entity>

<entity name="NonStrictDelayInitialized" class="ptolemy.domains.sr.lib.NonStrictDelayInitialized">
</entity>

<entity name="NonStrictDisplay" class="ptolemy.domains.sr.lib.NonStrictDisplay">
</entity>

<entity name="NonStrictLogicFunction" class="ptolemy.domains.sr.lib.NonStrictLogicFunction">
    <property name="function">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="and" value="and"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="or" value="or"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="xor" value="xor"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="nand" value="nand"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="nor" value="nor"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="xnor" value="xnor"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
</entity>

<entity name="NonStrictThreeBitAdder" class="ptolemy.domains.sr.lib.NonStrictThreeBitAdder">
</entity>

<entity name="SingleTokenDistributor" class="ptolemy.domains.sr.lib.SingleTokenDistributor">
</entity>

<entity name="Undefined" class="ptolemy.domains.sr.lib.Undefined">
    <property name="outputType">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="boolean" value="boolean"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="complex" value="complex"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="double" value="double"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="fix" value="fix"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="int" value="int"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="long" value="long"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="string" value="string"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
</entity>

      </group>
    ?>
  </configure>
</entity>

