<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">

<group>
  <entity name="Utilities" class="ptolemy.moml.EntityLibrary">
    <input source="ptolemy/configs/basicUtilities.xml"/>
  </entity>
  <entity name="Directors" class="ptolemy.moml.EntityLibrary">
    <configure>
      <?moml
        <group>
          <input source="ptolemy/configs/basicDirectors.xml"/>
          <input source="ptolemy/configs/experimentalDirectors.xml"/>
        </group>
      ?>
    </configure>
  </entity>
  <entity name="Attributes" class="ptolemy.moml.EntityLibrary">
    <property name="HierarchyFlattening" class="ptolemy.actor.gt.HierarchyFlatteningAttribute"/>
    <property name="RelationCollapsing" class="ptolemy.actor.gt.RelationCollapsingAttribute"/>
  </entity>
  <entity name="CompositeActorMatcher" class="ptolemy.actor.gt.CompositeActorMatcher"/>
  <entity name="AtomicActorMatcher" class="ptolemy.actor.gt.AtomicActorMatcher"/>
</group>