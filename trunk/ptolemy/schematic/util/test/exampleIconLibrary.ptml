<?xml version="1.0" standalone="no"?>
<!DOCTYPE iconlibrary SYSTEM "ptolemyicon.dtd">

<iconlibrary name="SDF" version="1.0">
<description>Icons for use within SDF</description>

<icon name="LoadImage">
<description>Load an image from a file</description>
<parameter name="filename" type="string"/>
<port name="image" output="true" type="doubleArray"/>
<xmlgraphic>
<rectangle color="red" fill="red" coords="-20 -20 40 40"/>
<!-- ovals are described in terms of a bounding box -->
<text font="helvetica" coords="0 0">Hello!</text>
<polygon coords="0 0 30 30 0 30 30 0" fill="blue"/>
<ellipse fill="yellow" coords="0 0 10 10"/>
</xmlgraphic>
</icon>

<icon name="SaveImage">
<port name="filename" input="true" type="string"/>
<port name="image" input="true" type="doubleArray"/>
<xmlgraphic>
<rectangle outline="red" fill="blue" coords="0 0 10 10"/>
</xmlgraphic>
</icon>
</iconlibrary>

