<?xml version="1.0" standalone="no"?>
<!DOCTYPE model SYSTEM "../plotml.dtd">
<plot>
  <noGrid/>
  <noColor/>
  <title>Title Goes Here</title>
  <wrap/>
  <xLabel>x axis</xLabel>
  <xRange min="0" max="100"/>
  <xTicks>
     <tick label="zero" position="0"/>
     <tick label="half way" position="50"/>
     <tick label="all the way" position="100"/>
  </xTicks>
  <yLabel>y axis</yLabel>
  <yRange min="-1" max="1"/>
  <yTicks>
     <tick label="one" position="1"/>
     <tick label="zero" position="0"/>
     <tick label="minus one" position="-1"/>
  </yTicks>
</plot>