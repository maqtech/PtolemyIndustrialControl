<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="StockReader" class="ptolemy.actor.TypedCompositeActor">
    <property name="_createdBy" class="ptolemy.kernel.util.VersionAttribute" value="2.1-devel"/>
    <doc> Read a stock quote and plot it </doc>
    <property name="stockName" class="ptolemy.data.expr.Parameter" value="&quot;YHOO&quot;">
    </property>
    <property name="SDFDirector" class="ptolemy.domains.sdf.kernel.SDFDirector">
        <property name="Scheduler" class="ptolemy.domains.sdf.kernel.SDFScheduler">
        </property>
        <property name="iterations" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="vectorizationFactor" class="ptolemy.data.expr.Parameter" value="1">
        </property>
    </property>
    <entity name="subscriber" class="ptolemy.actor.lib.jspaces.Subscriber">
        <property name="jspaceName" class="ptolemy.data.expr.Parameter" value="&quot;JavaSpaces&quot;">
        </property>
        <property name="entryName" class="ptolemy.data.expr.Parameter" value="stockName">
        </property>
        <property name="blocking" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="defaultToken" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="trigger" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <entity name="display" class="ptolemy.actor.lib.gui.SequencePlotter">
        <property name="fillOnWrapup" class="ptolemy.data.expr.Parameter" value="true">
        </property>
        <property name="legend" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="startingDataset" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="xInit" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="xUnit" class="ptolemy.data.expr.Parameter" value="1.0">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
        <configure source="plotConfigure.xml"><?xml version="1.0" standalone="no"?>
        <!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD PlotML 1//EN"
             "http://ptolemy.eecs.berkeley.edu/xml/dtd/PlotML_1.dtd">
        <plot>
          <size width="450" height="300"/>
          <title>Stock Quote Plot</title>
        </plot>
        </configure>
    </entity>
    <relation name="r1" class="ptolemy.actor.TypedIORelation">
    </relation>
    <link port="subscriber.output" relation="r1"/>
    <link port="display.input" relation="r1"/>
</entity>
