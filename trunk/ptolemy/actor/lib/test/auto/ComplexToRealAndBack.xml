<?xml version="1.0" standalone="no"?>
<!DOCTYPE model PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<model name="" class="ptolemy.actor.TypedCompositeActor">
    <director name="SDF" class="ptolemy.domains.sdf.kernel.SDFDirector">
        <property name="iterations" class="ptolemy.data.expr.Parameter" value="3">
        </property>
        <property name="vectorizationFactor" class="ptolemy.data.expr.Parameter" value="1">
        </property>
    </director>
    <entity name="Ramp0" class="ptolemy.actor.lib.Ramp">
        <property name="firingCountLimit" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="init" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="step" class="ptolemy.data.expr.Parameter" value="0.1 - 0.1*i">
        </property>
        <rendition name="_icon" class="ptolemy.vergil.toolbox.XMLIcon">
            <location value="43.0, 93.0"/>
            <configure>
            <![CDATA[
                <xmlgraphic>
                <rectangle coords="0 0 60 40" fill="white"/>
                <polygon coords="10 30 50 10 50 30" fill="blue"/>
                </xmlgraphic>
            ]]>
            </configure>
        </rendition>
        <doc>Create a sequence of tokens with increasing value</doc>
        <property name="editorFactory0" class="ptolemy.actor.gui.EditorPaneFactory">
        </property>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="trigger" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <entity name="ComplexToReal1" class="ptolemy.actor.lib.conversions.ComplexToReal">
        <rendition name="_icon" class="ptolemy.vergil.toolbox.LibraryIcon">
            <location value="149.0, 97.0"/>
            <configure>generic.transformer</configure>
        </rendition>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="real" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="imag" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="RealToComplex2" class="ptolemy.actor.lib.conversions.RealToComplex">
        <rendition name="_icon" class="ptolemy.vergil.toolbox.LibraryIcon">
            <location value="266.0, 98.0"/>
            <configure>generic.transformer</configure>
        </rendition>
        <port name="real" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="imag" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="Test3" class="ptolemy.actor.lib.Test">
        <property name="correctValues" class="ptolemy.data.expr.Parameter" value="[0.0 + 0.0i, 0.1 - 0.1i, 0.2 - 0.2i]">
        </property>
        <property name="tolerance" class="ptolemy.data.expr.Parameter" value="1.0E-9">
        </property>
        <rendition name="_icon" class="ptolemy.vergil.toolbox.LibraryIcon">
            <location value="384.0, 102.0"/>
            <configure>generic.sink</configure>
        </rendition>
        <property name="editorFactory0" class="ptolemy.actor.gui.EditorPaneFactory">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <relation name="relation5" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="relation7" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="relation9" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="relation11" class="ptolemy.actor.TypedIORelation">
    </relation>
    <link port="Ramp0.output" relation="relation5"/>
    <link port="ComplexToReal1.input" relation="relation5"/>
    <link port="ComplexToReal1.real" relation="relation7"/>
    <link port="ComplexToReal1.imag" relation="relation9"/>
    <link port="RealToComplex2.real" relation="relation7"/>
    <link port="RealToComplex2.imag" relation="relation9"/>
    <link port="RealToComplex2.output" relation="relation11"/>
    <link port="Test3.input" relation="relation11"/>
</model>
