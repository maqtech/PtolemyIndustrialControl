<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="generic" class="ptolemy.moml.TypedActorLibrary">
  <doc>Generic domain-polymorphic actors</doc>
  <configure>
    <?moml
      <group>
        <input source="sources.xml"/>
        <input source="sinks.xml"/>
        <input source="transformers.xml"/>
        <input source="conversions/conversions.xml"/>
        <entity name="TypedCompositeActor"
                class="ptolemy.actor.TypedCompositeActor"/>
        <entity name="FSMActor" class="ptolemy.domains.fsm.kernel.FSMActor">
           <property name="notation"
                     class="ptolemy.vergil.ptolemy.fsm.PtolemyFSMNotation"/>
        </entity>
      </group>
    ?>
  </configure>
</entity>
