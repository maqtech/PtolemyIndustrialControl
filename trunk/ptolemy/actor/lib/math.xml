<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="math" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
  <doc>Arithmetic and mathematical functions</doc>

  <entity name="AbsoluteValue" class="ptolemy.actor.lib.AbsoluteValue">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>
 
  <entity name="AddSubtract" class="ptolemy.actor.lib.AddSubtract">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
       <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="Average" class="ptolemy.actor.lib.Average">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="Expression" class="ptolemy.actor.lib.Expression">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>
  
  <entity name="MathFunction" class="ptolemy.actor.lib.MathFunction">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
    <property name="function">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="exp" value="exp"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="log" value="log"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="remainder" value="remainder"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="sign" value="sign"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="square" value="square"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="sqrt" value="sqrt"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
  </entity>


  <entity name="Maximum" class="ptolemy.actor.lib.Maximum">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="Minimum" class="ptolemy.actor.lib.Minimum">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="MultiplyDivide" class="ptolemy.actor.lib.MultiplyDivide">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
       <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="Quantizer" class="ptolemy.actor.lib.Quantizer">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>
  
  <entity name="Scale" class="ptolemy.actor.lib.Scale">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="SquareRoot" class="ptolemy.actor.lib.SquareRoot">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
  </entity>

  <entity name="TrigFunction" class="ptolemy.actor.lib.TrigFunction">
    <rendition class="ptolemy.vergil.toolbox.LibraryIcon">
      <configure>generic.transformer</configure>
    </rendition>
    <property name="function">
        <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
            <property name="acos" value="acos"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="asin" value="asin"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="atan" value="atan"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="cos" value="cos"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="sin" value="sin"
	              class="ptolemy.kernel.util.StringAttribute"/>
            <property name="tan" value="tan"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
  </entity>

      </group>
    ?>
  </configure>
</entity>
