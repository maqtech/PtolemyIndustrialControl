<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="ResourceScheduler" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
  <doc>Resource Schedulers</doc>

 
<entity name="FixedPriorityScheduler" class="ptolemy.actor.lib.resourceScheduler.FixedPriorityScheduler"> </entity>
<entity name="FCFSScheduler" class="ptolemy.actor.lib.resourceScheduler.FCFSScheduler"> </entity>
<entity name="EDFScheduler" class="ptolemy.actor.lib.resourceScheduler.EDFScheduler"> </entity>
<entity name="DynamicCoreAssignmentScheduler" class="ptolemy.actor.lib.resourceScheduler.DynamicCoreAssignmentScheduler"> </entity>

<entity name="CompositeResourceScheduler" class="ptolemy.actor.lib.resourceScheduler.CompositeResourceScheduler"> 
<property name="DE Director" class="ptolemy.domains.de.kernel.DEDirector">
    <property name="_location" class="ptolemy.kernel.util.Location" value="[80.0, 25.0]">
    </property>
</property>

<entity name="ResourceMappingInputPort" class="ptolemy.actor.lib.resourceScheduler.ResourceMappingInputPort">
<property name="_location" class="ptolemy.kernel.util.Location" value="[40.0, 200.0]">
    </property>
</entity>

<entity name="ResourceMappingOutputPort" class="ptolemy.actor.lib.resourceScheduler.ResourceMappingOutputPort">
	<property name="_location" class="ptolemy.kernel.util.Location" value="[200.0, 200.0]">
    </property>
	<property name="DEDirector" class="ptolemy.domains.de.kernel.DEDirector"/>
	<property createIfNecessary="true" name="Parameter" class="ptolemy.data.expr.Parameter"></property>
	<port name="input" class="ptolemy.actor.TypedIOPort">
    	<property name="input"/>
	</port>
	<entity name="SetVariable" class="ptolemy.actor.lib.SetVariable">
		<property name="variableName" class="ptolemy.kernel.util.StringAttribute" value="Parameter">
    	</property>
    	<property name="delayed" class="ptolemy.data.expr.Parameter" value="false">
    	</property>
    	<port name="input" class="ptolemy.actor.TypedIOPort">
        	<property name="input"/>
    	</port>
    	<port name="output" class="ptolemy.actor.TypedIOPort">
        	<property name="output"/>
    	</port>
	</entity>
	<relation name="relation" class="ptolemy.actor.TypedIORelation">
	</relation>
    <link port="input" relation="relation"/>
    <link port="SetVariable.input" relation="relation"/>
</entity>
</entity>

<entity name="ResourceMappingInputPort" class="ptolemy.actor.lib.resourceScheduler.ResourceMappingInputPort">
</entity>

<entity name="ResourceMappingOutputPort" class="ptolemy.actor.lib.resourceScheduler.ResourceMappingOutputPort">
	<property name="DEDirector" class="ptolemy.domains.de.kernel.DEDirector"/>
	<property createIfNecessary="true" name="Parameter" class="ptolemy.data.expr.Parameter"></property>
	<port name="input" class="ptolemy.actor.TypedIOPort">
    	<property name="input"/>
	</port>
	<entity name="SetVariable" class="ptolemy.actor.lib.SetVariable">
		<property name="variableName" class="ptolemy.kernel.util.StringAttribute" value="Parameter">
    	</property>
    	<property name="delayed" class="ptolemy.data.expr.Parameter" value="false">
    	</property>
    	<port name="input" class="ptolemy.actor.TypedIOPort">
        	<property name="input"/>
    	</port>
    	<port name="output" class="ptolemy.actor.TypedIOPort">
        	<property name="output"/>
    	</port>
	</entity>
	<relation name="relation" class="ptolemy.actor.TypedIORelation">
	</relation>
    <link port="input" relation="relation"/>
    <link port="SetVariable.input" relation="relation"/>
</entity>
      </group>
    ?>
  </configure>
</entity>


