            <property name="round" value="round"
	              class="ptolemy.kernel.util.StringAttribute"/>
	        <property name="truncate" value="truncate"
	              class="ptolemy.kernel.util.StringAttribute"/>
        </property>
     </property>
  </entity>


<entity name="SequenceToArray" class="ptolemy.domains.sdf.lib.SequenceToArray">
</entity>

      </group>
    ?>
