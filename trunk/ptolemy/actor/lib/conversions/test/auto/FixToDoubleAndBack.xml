<?xml version="1.0" standalone="no"?>
<!DOCTYPE model PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<model name="" class="ptolemy.actor.TypedCompositeActor">
    <property name="SDF" class="ptolemy.domains.sdf.kernel.SDFDirector">
        <property name="iterations" class="ptolemy.data.expr.Parameter" value="3">
        </property>
        <property name="vectorizationFactor" class="ptolemy.data.expr.Parameter" value="1">
        </property>
    </property>
    <entity name="Ramp0" class="ptolemy.actor.lib.Ramp">
        <property name="firingCountLimit" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <property name="init" class="ptolemy.data.expr.Parameter" value="1">
        </property>
        <property name="step" class="ptolemy.data.expr.Parameter" value=".25">
        </property>
        <rendition name="_icon" class="ptolemy.vergil.toolbox.XMLIcon">
            <location value="55.0, 111.0"/>
            <configure>
            <![CDATA[
                <xmlgraphic>
                <rectangle coords="0 0 60 40" fill="white"/>
                <polygon coords="10 30 50 10 50 30" fill="blue"/>
                </xmlgraphic>
            ]]>
            </configure>
        </rendition>
        <doc>Create a sequence of tokens with increasing value</doc>
        <property name="editorFactory0" class="ptolemy.actor.gui.EditorPaneFactory">
        </property>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
        <port name="trigger" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <entity name="Test1" class="ptolemy.actor.lib.Test">
        <property name="correctValues" class="ptolemy.data.expr.Parameter" value="[1, 1.25, 1.5]">
        </property>
        <property name="tolerance" class="ptolemy.data.expr.Parameter" value="1.0E-9">
        </property>
        <rendition name="_icon" class="ptolemy.vergil.toolbox.LibraryIcon">
            <location value="404.0, 113.0"/>
            <configure>generic.sink</configure>
        </rendition>
        <property name="editorFactory0" class="ptolemy.actor.gui.EditorPaneFactory">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
            <property name="multiport"/>
        </port>
    </entity>
    <entity name="FixToDouble2" class="ptolemy.actor.lib.conversions.FixToDouble">
        <property name="precision" class="ptolemy.data.expr.Parameter" value="&quot;16/4&quot;">
        </property>
        <property name="overflow" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <rendition name="_icon" class="ptolemy.vergil.toolbox.LibraryIcon">
            <location value="302.0, 112.0"/>
            <configure>generic.transformer</configure>
        </rendition>
        <property name="editorFactory0" class="ptolemy.actor.gui.EditorPaneFactory">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <entity name="DoubleToFix3" class="ptolemy.actor.lib.conversions.DoubleToFix">
        <property name="precision" class="ptolemy.data.expr.Parameter" value="&quot;(16/4)&quot;">
        </property>
        <property name="quantizer" class="ptolemy.data.expr.Parameter" value="0">
        </property>
        <rendition name="_icon" class="ptolemy.vergil.toolbox.LibraryIcon">
            <location value="173.0, 110.0"/>
            <configure>generic.transformer</configure>
        </rendition>
        <property name="editorFactory0" class="ptolemy.actor.gui.EditorPaneFactory">
        </property>
        <port name="input" class="ptolemy.actor.TypedIOPort">
            <property name="input"/>
        </port>
        <port name="output" class="ptolemy.actor.TypedIOPort">
            <property name="output"/>
        </port>
    </entity>
    <relation name="relation5" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="relation7" class="ptolemy.actor.TypedIORelation">
    </relation>
    <relation name="relation9" class="ptolemy.actor.TypedIORelation">
    </relation>
    <link port="Ramp0.output" relation="relation5"/>
    <link port="Test1.input" relation="relation9"/>
    <link port="FixToDouble2.input" relation="relation7"/>
    <link port="FixToDouble2.output" relation="relation9"/>
    <link port="DoubleToFix3.input" relation="relation5"/>
    <link port="DoubleToFix3.output" relation="relation7"/>
</model>
