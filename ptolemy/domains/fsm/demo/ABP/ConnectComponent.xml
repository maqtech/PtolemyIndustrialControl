    <property name="_nonStrictMarker" class="ptolemy.kernel.util.Attribute">
    </property>
    <property name="_createdBy" class="ptolemy.kernel.util.VersionAttribute" value="2.1-devel">
    </property>
    <property name="count" class="ptolemy.data.expr.Parameter" value="3">
    </property>
    <property name="_vergilSize" class="ptolemy.actor.gui.SizeAttribute" value="[817, 663]">
    </property>
    <property name="_vergilLocation" class="ptolemy.actor.gui.LocationAttribute" value="[-4, -4]">
    </property>
    <port name="request" class="ptolemy.actor.TypedIOPort">
        <property name="input"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="25.0, 85.0">
        </property>
    </port>
    <port name="connected" class="ptolemy.actor.TypedIOPort">
        <property name="output"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="755.0, 135.0">
        </property>
    </port>
    <port name="error" class="ptolemy.actor.TypedIOPort">
        <property name="output"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="755.0, 180.0">
        </property>
    </port>
    <port name="ack" class="ptolemy.actor.TypedIOPort">
        <property name="input"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="int">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="25.0, 125.0">
        </property>
    </port>
    <port name="pktOut" class="ptolemy.actor.TypedIOPort">
        <property name="output"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="int">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="755.0, 40.0">
        </property>
    </port>
    <port name="setTimer" class="ptolemy.actor.TypedIOPort">
        <property name="output"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="double">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="755.0, 90.0">
        </property>
    </port>
    <port name="expired" class="ptolemy.actor.TypedIOPort">
        <property name="input"/>
        <property name="type" class="ptolemy.actor.TypeAttribute" value="general">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="25.0, 40.0">
        </property>
    </port>
    <entity name="Init" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="150.0, 75.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <entity name="Wait" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="335.0, 155.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <entity name="Success" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="590.0, 140.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <entity name="Fail" class="ptolemy.domains.fsm.kernel.State">
        <property name="refinementName" class="ptolemy.kernel.util.StringAttribute">
        </property>
        <property name="_location" class="ptolemy.kernel.util.Location" value="550.0, 370.0">
        </property>
        <port name="incomingPort" class="ptolemy.kernel.ComponentPort">
        </port>
        <port name="outgoingPort" class="ptolemy.kernel.ComponentPort">
        </port>
    </entity>
    <relation name="conTr1" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="request_isPresent">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute" value="pktOut = -1; setTimer = 5.0">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute" value="count = 5">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="1.1036397104525355">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="0.5316255117860138">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <relation name="conTr2" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="ack_isPresent &amp;&amp; ack == -1">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute" value="connected = true; setTimer = -1.0">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="PI/5.0">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <relation name="conTr3" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="!ack_isPresent &amp;&amp; expired_isPresent &amp;&amp; count == 0">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute" value="error = true">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="PI/5.0">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="0.0">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <relation name="conTr4" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="ack_isPresent &amp;&amp; ack != -1 &amp;&amp; expired_isPresent &amp;&amp; count == 0">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute" value="error = true">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="1.3141196704830953">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="0.4802236330299203">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <relation name="conTr5" class="ptolemy.domains.fsm.kernel.Transition">
        <property name="guardExpression" class="ptolemy.kernel.util.StringAttribute" value="(!ack_isPresent || ack != -1) &amp;&amp; expired_isPresent &amp;&amp; count != 0">
        </property>
        <property name="outputActions" class="ptolemy.domains.fsm.kernel.OutputActionsAttribute" value="pktOut = -1; setTimer = 5.0">
        </property>
        <property name="setActions" class="ptolemy.domains.fsm.kernel.CommitActionsAttribute" value="count = count - 1">
        </property>
        <property name="exitAngle" class="ptolemy.data.expr.Parameter" value="2.1566028432913193">
        </property>
        <property name="gamma" class="ptolemy.data.expr.Parameter" value="3.1182483768189058">
        </property>
        <property name="reset" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="preemptive" class="ptolemy.data.expr.Parameter" value="false">
        </property>
        <property name="triggerExpression" class="ptolemy.kernel.util.StringAttribute">
        </property>
    </relation>
    <link port="Init.outgoingPort" relation="conTr1"/>
    <link port="Wait.incomingPort" relation="conTr1"/>
    <link port="Wait.incomingPort" relation="conTr5"/>
    <link port="Wait.outgoingPort" relation="conTr2"/>
    <link port="Wait.outgoingPort" relation="conTr3"/>
    <link port="Wait.outgoingPort" relation="conTr4"/>
    <link port="Wait.outgoingPort" relation="conTr5"/>
    <link port="Success.incomingPort" relation="conTr2"/>
    <link port="Fail.incomingPort" relation="conTr3"/>
    <link port="Fail.incomingPort" relation="conTr4"/>
</class>
