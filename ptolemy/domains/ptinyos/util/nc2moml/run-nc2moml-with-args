#!/bin/sh

if [ -z $TOSROOT ]; then
  echo "Error: \$TOSROOT must be defined"
  exit 0
fi
TOS_INPUT_DIR=$TOSROOT

if [ -z $PTII ]; then
  echo "Error: \$PTII must be defined"
fi

MOML_OUTPUT_DIR=$PTII/vendors/ptinyos/moml

DIR_LIST=tos/lib/Counters

echo "./run-nc2moml $TOS_INPUT_DIR \\\''\$CLASSPATH'\\\' $MOML_OUTPUT_DIR $DIR_LIST"

./run-nc2moml $TOS_INPUT_DIR \\\''\$CLASSPATH'\\\' $MOML_OUTPUT_DIR $DIR_LIST
