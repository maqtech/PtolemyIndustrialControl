<!ENTITY % inlines "(doc | manager | director | parameter | actor |
	 connection | link | placement | icon)*">
<!ELEMENT model %inlines;>
<!ATTLIST model name CDATA "top">
<!ATTLIST model class CDATA "ptolemy.actor.TypedCompositeActor">

  <!ELEMENT doc (#PCDATA)>

  <!ELEMENT actor %inlines;>
  <!ATTLIST actor name CDATA #REQUIRED>
  <!ATTLIST actor class CDATA "ptolemy.actor.TypedCompositeActor">

  <!ELEMENT director (parameter*)>
  <!ATTLIST director name CDATA "director">
  <!ATTLIST director class CDATA #REQUIRED>

  <!ELEMENT connection (vertex*)>
  <!ATTLIST connection name CDATA #IMPLIED>
  <!ATTLIST connection port1 CDATA #REQUIRED>
  <!ATTLIST connection port2 CDATA #REQUIRED>

  <!ELEMENT link (vertex*)>
  <!ATTLIST link port CDATA #REQUIRED>
  <!ATTLIST link connection CDATA #REQUIRED>
  <!ATTLIST link vertex CDATA #IMPLIED>

    <!ELEMENT vertex EMPTY>
    <!ATTLIST vertex name CDATA "">
    <!ATTLIST vertex xpos CDATA #IMPLIED>
    <!ATTLIST vertex ypos CDATA #IMPLIED>

  <!ELEMENT parameter EMPTY>
  <!ATTLIST parameter name CDATA #REQUIRED>
  <!ATTLIST parameter value CDATA #REQUIRED>

  <!ELEMENT icon EMPTY>
  <!ATTLIST icon class CDATA "ptolemy.actor.gui.GenericIcon">
  <!ATTLIST icon xpos CDATA #IMPLIED>
  <!ATTLIST icon ypos CDATA #IMPLIED>
  <!ATTLIST icon modifiers CDATA #IMPLIED>
