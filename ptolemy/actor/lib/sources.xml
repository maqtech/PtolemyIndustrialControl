<?xml version="1.0" standalone="no"?>
<!DOCTYPE model SYSTEM "../../moml/moml.dtd">

<model name="sources" class="ptolemy.actor.TypedCompositeActor">

<class name="actors" extends="ptolemy.actor.TypedCompositeActor">
<doc>Generic domain polymorphic sources</doc>

<!-- Sources -->
<entity name="bernoulli" class="ptolemy.actor.lib.Bernoulli">
<rendition class="ptolemy.schematic.util.LibraryIcon">
<configure>generic.source</configure>
</rendition>
<doc>Create a sequence of bernoulli random variables</doc>
</entity>

<entity name="clock" class="ptolemy.actor.lib.Clock">
<rendition class="ptolemy.schematic.util.LibraryIcon">
<configure>generic.source</configure>
</rendition>
<doc>Create a generalized square wave</doc>
</entity>

<entity name="const" class="ptolemy.actor.lib.Const">
<rendition class="ptolemy.schematic.util.LibraryIcon">
<configure>generic.source</configure>
</rendition>
<doc>Create a constant sequence</doc>
</entity>

<entity name="gaussian" class="ptolemy.actor.lib.Gaussian">
<rendition class="ptolemy.schematic.util.LibraryIcon">
<configure>generic.source</configure>
</rendition>
<doc>Create a sequence of gaussian random variables</doc>
</entity>

<entity name="ramp" class="ptolemy.actor.lib.Ramp">
<rendition class="ptolemy.schematic.util.LibraryIcon">
<configure>generic.source</configure>
</rendition><doc>Create a sequence of tokens with increasing value</doc>
</entity>
</class>
</model>

