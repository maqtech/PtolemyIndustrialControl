<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<entity name="jai actors" class="ptolemy.moml.EntityLibrary">
  <configure>
    <?moml
      <group>
        <doc>Java Advanced Imaging actors</doc>
	<entity name="JAIBMPWriter" class="ptolemy.actor.lib.jai.JAIBMPWriter">
	</entity>
	
	<entity name="JAIDataCaster" class="ptolemy.actor.lib.jai.JAIDataCaster">
	  <property name="dataFormat">
	      <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
	          <property name="byte" value="byte"
	      	            class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="double" value="double"
			    class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="float" value="float"
		            class="ptolemy.kernel.util.StringAttribute"/>
                  <property name="int" value="int"
	                    class="ptolemy.kernel.util.StringAttribute"/>
                  <property name="short" value="short"
	                    class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="ushort" value="ushort"
	                    class="ptolemy.kernel.util.StringAttribute"/>
	      </property>   
	  </property>
	</entity>
	
	<entity name="JAIDCT" class="ptolemy.actor.lib.jai.JAIDCT">
	</entity>
	<entity name="JAIDFT" class="ptolemy.actor.lib.jai.JAIDFT">
	  <property name="scalingType">
	      <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
	          <property name="dimensions" value="dimensions"
	      	            class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="unitary" value="unitary"
			    class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="none" value="none"
		            class="ptolemy.kernel.util.StringAttribute"/>
	      </property>   
	  </property>
	  <property name="dataNature">
	      <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
	          <property name="complexToComplex" value="complexToComplex"
	      	            class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="complexToReal" value="complexToReal"
			    class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="realToComplex" value="realToComplex"
		            class="ptolemy.kernel.util.StringAttribute"/>
	      </property>   
	  </property>
	</entity>
	<entity name="JAIEdgeDetection" class="ptolemy.actor.lib.jai.JAIEdgeDetection">
	</entity>
	<entity name="JAIIDCT" class="ptolemy.actor.lib.jai.JAIIDCT">
	</entity>
	<entity name="JAIIDFT" class="ptolemy.actor.lib.jai.JAIIDFT">
	</entity>
	<entity name="JAIImageReader" class="ptolemy.actor.lib.jai.JAIImageReader">
	</entity>
	<entity name="JAIInvert" class="ptolemy.actor.lib.jai.JAIInvert">
	</entity>
	<entity name="JAIJPEGWriter" class="ptolemy.actor.lib.jai.JAIJPEGWriter">
	</entity>
	<entity name="JAIPNMWriter" class="ptolemy.actor.lib.jai.JAIPNMWriter">
	</entity>
	<entity name="JAIScale" class="ptolemy.actor.lib.jai.JAIScale">
	  <property name="interpolationType">
	      <property name="style" class="ptolemy.actor.gui.style.ChoiceStyle">
	          <property name="bicubic" value="bicubic"
	      	            class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="bicubic2" value="bicubic2"
			    class="ptolemy.kernel.util.StringAttribute"/>
	          <property name="bilinear" value="bilinear"
		            class="ptolemy.kernel.util.StringAttribute"/>
                  <property name="nearestNeighbor" value="nearestNeighbor"
		            class="ptolemy.kernel.util.StringAttribute"/>
	      </property>   
	  </property>
	</entity>	
	<entity name="JAITIFFWriter" class="ptolemy.actor.lib.jai.JAITIFFWriter">
	</entity>
      </group>
    ?>
  </configure>
</entity>
