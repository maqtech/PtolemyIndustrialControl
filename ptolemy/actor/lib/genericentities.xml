<?xml version="1.0" standalone="no"?>
<!DOCTYPE model SYSTEM "../../moml/moml.dtd">
<class name="generic" extends="ptolemy.actor.TypedCompositeActor">
  <doc>
	Generic domain polymorphic actors
  </doc>
  <import source="sources.xml"/>
  <import source="sinks.xml"/>
  <import source="transformers.xml"/>

  <entity name="sources" class=".sources"/>
  <entity name="sinks" class=".sinks"/>
  <entity name="transformers" class=".transformers"/>

  <entity name="TypedCompositeActor" class="ptolemy.actor.TypedCompositeActor"/>
  <entity name="FSMActor" class="ptolemy.domains.fsm.kernel.FSMActor">
  <property name="notation" class="ptolemy.vergil.ptolemy.fsm.PtolemyFSMNotation"/>
  </entity> 
  <entity name="IOPort" class="ptolemy.actor.TypedIOPort"/>
</class>
