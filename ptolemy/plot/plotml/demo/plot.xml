<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot SYSTEM "../plotml.dtd">
<plot>
  <noGrid/>
  <title>Title Goes Here</title>
  <xLabel>x axis</xLabel>
  <xRange min="0" max="5"/>
  <yLabel>y axis</yLabel>
  <yRange min="-1" max="1"/>
  <barGraph width="0.25" offset="0.1"/>
  <default marks="dots" stems="yes"/>

  <dataset name="first">
    <p x="0" y="1"/>
    <p x="1" y="0.5"/>
    <point x="2" y="0.1"/>
    <move x="3" y="-0.5"/>
    <p x="4" y="-0.8" lowErrorBar="-1.0" highErrorBar="-0.6"/>
    <m x="5" y="0.5"/>
  </dataset>
  <dataset name="second set" marks="none" connected="no">
    <p x="0" y="-0.5"/>
    <p x="1" y="-0.5"/>
    <point x="2" y="-0.1"/>
    <move x="3" y="0.5"/>
    <p x="4" y="0.8" lowErrorBar="1.0" highErrorBar="0.6"/>
    <m x="5" y="-0.5"/>
  </dataset>
  <dataset name="third" stems="no">
    <p x="0" y="-1.0"/>
    <p x="1" y="-0.5"/>
    <p x="2" y="0.0"/>
    <p x="3" y="0.5"/>
    <p x="4" y="1.0"/>
    <p x="5" y="0.5"/>
  </dataset>

</plot>