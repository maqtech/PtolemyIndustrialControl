<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">

<property name="ModelID" class="ptolemy.kernel.attributes.IDAttribute">
    <property name="_icon" class="ptolemy.vergil.icon.BoxedValuesIcon">
        <property name="attributeName" class="ptolemy.kernel.util.StringAttribute" value="name">
        </property>
        <property name="displayWidth" class="ptolemy.data.expr.Parameter" value="80">
        </property>
    </property>
    <!-- prevent the name from being editable, since SaveAs is the naming mechanism. -->
    <property name="name">
        <property name="style" class="ptolemy.actor.gui.style.NotEditableLineStyle"/>
    </property>
</property>