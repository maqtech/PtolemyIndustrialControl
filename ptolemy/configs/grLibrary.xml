<?xml version="1.0" standalone="no"?>
<!DOCTYPE entity PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<property name="_library" class="ptolemy.moml.LibraryAttribute">
  <configure>
    <entity name="graph transformation library" class="ptolemy.kernel.CompositeEntity">
      <input source="ptolemy/configs/basicUtilities.xml"/>
      <entity name="CompositeActorMatcher" class="ptolemy.actor.gr.CompositeActorMatcher">
        <input source="ptolemy/configs/grLibrary.xml"/>
      </entity>
      <entity name="AtomicActorMatcher" class="ptolemy.actor.gr.AtomicActorMatcher">
        <property name="_editorFactory" class="ptolemy.actor.gr.RuleEditor$Factory">
          <property name="attributeName" value="ruleList"/>
        </property>
      </entity>
    </entity>
  </configure>
</property>
