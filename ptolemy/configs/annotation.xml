<?xml version="1.0" standalone="no"?>
<!DOCTYPE plot PUBLIC "-//UC Berkeley//DTD MoML 1//EN"
    "http://ptolemy.eecs.berkeley.edu/xml/dtd/MoML_1.dtd">
<group>
<property name="annotation" class="ptolemy.kernel.util.Attribute">
  <property name="_hideName" class="ptolemy.kernel.util.SingletonAttribute"/>
  <property name="_iconDescription" class="ptolemy.kernel.util.SingletonConfigurableAttribute">
    <configure>
      <svg>
        <text x="20" style="font-size:14; font-family:SansSerif; fill:blue" y="20">Double click to edit text.</text>
      </svg>
    </configure>
  </property>
  <property name="_smallIconDescription" class="ptolemy.kernel.util.SingletonConfigurableAttribute">
    <configure>
      <svg>
        <text x="20" style="font-size:14; font-family:SansSerif; fill:blue" y="20">-A-</text>
      </svg>
    </configure>
  </property>
  <!-- The following attribute specifies the context menu commands -->
  <property name="_controllerFactory" class="ptolemy.vergil.basic.NodeControllerFactory"/>
  <!-- The following attribute makes the right editor open -->
  <property name="_editorFactory" class="ptolemy.vergil.toolbox.AnnotationEditorFactory"/>
</property>
<property name="parameter" class="ptolemy.data.expr.Parameter">
  <property name="_hideName" class="ptolemy.kernel.util.SingletonAttribute"/>
  <property name="_icon" class="ptolemy.vergil.icon.ValueIcon"/>
  <property name="_smallIconDescription" class="ptolemy.kernel.util.SingletonConfigurableAttribute">
    <configure>
      <svg>
        <text x="20" style="font-size:14; font-family:SansSerif; fill:blue" y="20">-P-</text>
      </svg>
    </configure>
  </property>
  <property name="_editorFactory" class="ptolemy.vergil.toolbox.VisibleParameterEditorFactory"/>
</property>
<property name="SliderParameter" class="ptolemy.actor.parameters.SliderParameter">
</property>
<property name="portParameter" class="ptolemy.actor.parameters.PortParameter">
  <property name="_hideName" class="ptolemy.kernel.util.SingletonAttribute"/>
  <property name="_iconDescription" class="ptolemy.kernel.util.SingletonConfigurableAttribute">
    <configure>
      <svg>
	<polyline points="-15,-15, -3,-5, -16,5" style="stroke:black"/>
      </svg>
    </configure>
  </property>
  <property name="_icon" class="ptolemy.vergil.icon.ValueIcon"/>
  <property name="_smallIconDescription" class="ptolemy.kernel.util.SingletonConfigurableAttribute">
    <configure>
      <svg>
        <text x="20" style="font-size:14; font-family:SansSerif; fill:black" y="20">-P-</text>
      </svg>
    </configure>
  </property>
  <property name="_editorFactory" class="ptolemy.vergil.toolbox.VisibleParameterEditorFactory"/>
</property>
<property name="requireVersion" class="ptolemy.kernel.attributes.RequireVersion">
  <property name="_hideName" class="ptolemy.kernel.util.SingletonAttribute"/>
  <property name="_icon" class="ptolemy.vergil.icon.ValueIcon"/>
  <property name="_smallIconDescription" class="ptolemy.kernel.util.SingletonConfigurableAttribute">
    <configure>
      <svg>
        <text x="20" style="font-size:14; font-family:SansSerif; fill:blue" y="20">-V-</text>
      </svg>
    </configure>
  </property>
</property>
</group>
