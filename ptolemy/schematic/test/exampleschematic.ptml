<?xml version="1.0" standalone="no"?>
<!DOCTYPE schematic SYSTEM "ptolemyicon.dtd">

<schematic name="SDF" version="1.0">
<description>Icons for use within SDF</description>

<entity name="Load BMP File" icon="examplelibrary.LoadImage">
<description>Load the Image that will be transmitted and stored.</description>
<parameter name="filename" type="string"/>
<port name="image" output="true" type="doubleArray"/>
</entity>

<entity name="Save BMP File" icon="examplelibrary.SaveImage">
<parameter name="filename" type="string"/>
<port name="image" input="true" type="doubleArray"/>
</entity>

<relation name="R1">
<link name="Load BMP File.image"/>
<link name="Save BMP File.image"/>
</relation>

<!-- these parameters will get set in the Director -->
<parameter name="domain" value="SDF" type="string"/>
<parameter name="starttime" value="1.0" type="double"/>
<parameter name="endtime" value="7.0" type="double"/>

</schematic>

