<?xml version="1.0" standalone="no"?>
<!DOCTYPE entitylibrary SYSTEM "../../lib/ptml.dtd">

<entitylibrary name="SDF" version="1.0">
<description>Entities corresponding to SDF Actors</description>

<entity name="LoadImage" icon=".SDF.LoadImage" terminalstyle=".SDF.1out">
<description>Load an image from a file</description>
<terminalmap ports="output"/>
<parameter name="filename" value="test" type="string"/>
<port name="output" output="true" type="doubleArray"/>
</entity>

<entity name="SaveImage" icon=".SDF.SaveImage" terminalstyle=".SDF.1in">
<terminalmap ports="input"/>
<parameter name="filename" value="testout" type="string"/>
<port name="input" input="true" type="doubleArray"/>
</entity>
</entitylibrary>

