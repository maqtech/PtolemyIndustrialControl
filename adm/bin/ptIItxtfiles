#!/bin/sh
# -[Thu Jan 25 08:54:31 2001 by cxh]-
# Return all the text source files (with exceptions) in the ptII tree

$PTII/adm/bin/ptIIfiles | grep -v '\.#' | grep -v '\.bin; | grep -v .bmp \
	| grep -v .class | grep -v .fm \
	| grep -v .eps | grep -v .gif | grep -v .ico | grep -v .isj \
	| grep -v .jar | grep -v .jpg \
	| grep -v nqc | grep -v .psd \
	| grep -v .pdf | grep -v .ppt | grep -v .qcf | grep -v .vsd

